{
  "name": "@dashhub/backend",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "build:wait": "while [ ! -f ./dist/index.mjs ]; do sleep 0.5; done",
    "db:migrate": "node dist/run-migrations.cli.mjs",
    "es:reindex:all": "node dist/run-reindex-all.cli.mjs",
    "chats:summarize:all": "node dist/run-summarize-chats.cli.mjs",
    "projects:summarize:all": "node dist/run-summarize-projects.cli.mjs",
    "dev": "npm run clean && nodemon --watch src --watch ../../externals/commercial/backend/src/ -e ts --exec \"vite build --mode production && node --stack-trace-limit=1000 --enable-source-maps ./dist/index.mjs\"",
    "lint": "eslint . --config ./eslint.config.mjs",
    "clean": "rm -rf ./dist 2>/dev/null",
    "check:types": "tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "pg-native": ">=3.0.1"
  },
  "devDependencies": {
    "@dashhub/agents-library-sdk": "2.1.0",
    "@dashhub/sdk": "*",
    "@elastic/elasticsearch": "^8.17.0",
    "@google/generative-ai": "^0.24.0",
    "@hono/node-server": "^1.13.7",
    "@instructor-ai/instructor": "^1.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/mime-types": "^2.1.0",
    "@types/object-hash": "^3.0.6",
    "@types/pg": "^8.11.10",
    "@types/pg-cursor": "^2.7.2",
    "@types/utf-8-validate": "^5.0.2",
    "async-await-queue": "^2.1.4",
    "croner": "^9.0.0",
    "dotenv": "^16.4.7",
    "elastic-builder": "^2.31.0",
    "fast-deep-equal": "^3.1.3",
    "hono": "^4.6.17",
    "jwt-simple": "^0.5.6",
    "kysely": "^0.27.5",
    "mime-types": "^2.1.35",
    "minio": "^8.0.3",
    "object-hash": "^3.0.0",
    "officeparser": "^5.1.1",
    "openai": "^4.79.1",
    "pg": "^8.13.0",
    "pg-cursor": "^2.12.0",
    "rxjs": "^7.8.1",
    "tiny-glob": "^0.2.9",
    "tsyringe": "^4.8.0",
    "utf-8-validate": "^6.0.5",
    "winston": "^3.17.0"
  }
}
