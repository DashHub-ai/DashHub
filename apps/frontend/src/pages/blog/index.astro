---
import { Mail } from 'lucide-astro';

import ArticleCard from '../../components/blog/article-card.astro';
import DefaultLayout from '../../layouts/default-layout.astro';

const articles = [
  {
    title: 'The Joys of Owning a Cat',
    alt: 'Cat Playing with Toy',
    description: 'Discover the many joys and benefits of having a feline friend in your home.',
    href: '/blog/joys-of-owning-a-cat',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'Training Your Dog: Tips and Tricks',
    alt: 'Dog Training',
    description: 'Learn effective techniques to train your dog and build a strong bond.',
    href: '/blog/training-your-dog',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'The Best Pets for Small Apartments',
    alt: 'Pets in Small Apartments',
    description: 'Find out which pets are best suited for small living spaces.',
    href: '/blog/best-pets-for-small-apartments',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'How to Care for Your Pet Rabbit',
    alt: 'Pet Rabbit Care',
    description: 'A comprehensive guide to keeping your pet rabbit healthy and happy.',
    href: '/blog/care-for-your-pet-rabbit',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'Understanding Your Cat’s Behavior',
    alt: 'Cat Behavior',
    description: 'Decode the mysterious behaviors of your feline companion.',
    href: '/blog/understanding-cat-behavior',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'Top 10 Dog Breeds for Families',
    alt: 'Family Dog Breeds',
    description: 'Explore the best dog breeds that are perfect for families with children.',
    href: '/blog/top-10-dog-breeds-for-families',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'Creating a Pet-Friendly Home',
    alt: 'Pet-Friendly Home',
    description: 'Tips and ideas for making your home safe and comfortable for your pets.',
    href: '/blog/creating-a-pet-friendly-home',
    imageSrc: 'data:image/png;base64,',
  },
  {
    title: 'The Benefits of Adopting a Senior Pet',
    alt: 'Senior Pet Adoption',
    description: 'Learn about the advantages of giving a home to an older pet.',
    href: '/blog/benefits-of-adopting-senior-pet',
    imageSrc: 'data:image/png;base64,',
  },
];

const featuredArticles = [
  {
    title: 'The Joys of Owning a Cat',
    href: '/blog/joys-of-owning-a-cat',
  },
  {
    title: 'Training Your Dog: Tips and Tricks',
    href: '/blog/training-your-dog',
  },
  {
    title: 'The Best Pets for Small Apartments',
    href: '/blog/best-pets-for-small-apartments',
  },
  {
    title: 'How to Care for Your Pet Rabbit',
    href: '/blog/care-for-your-pet-rabbit',
  },
  {
    title: 'Understanding Your Cat’s Behavior',
    href: '/blog/understanding-cat-behavior',
  },
  {
    title: 'Top 10 Dog Breeds for Families',
    href: '/blog/top-10-dog-breeds-for-families',
  },
];
---
<DefaultLayout
  title="DashHub.ai"
  description="Welcome to Dashhub!"
>
  <section class="container mx-auto px-4 py-8">
    <div class="grid grid-cols-1 lg:grid-cols-3 gap-6 lg:gap-12">
      <div class="lg:col-span-2">
        <h2 class="text-2xl font-bold mb-4">Articles</h2>
        <div class="grid grid-cols-1 sm:grid-cols-2 gap-6 lg:gap-12">
          {articles.map(article => (
            <ArticleCard {...article} />
          ))}
        </div>
      </div>

      <aside class="lg:col-span-1">
        <h2 class="text-2xl font-bold mb-4">Featured Articles</h2>
        <ul class="flex flex-col gap-y-4 border-t border-gray-300 pt-4">
          {featuredArticles.map(article => (
            <li class="border-b border-gray-300">
              <a href={article.href} class="hover:underline block py-2 pb-4">
                {article.title}
              </a>
            </li>
          ))}
        </ul>

        <div class="mt-8 p-6 bg-gray-100 rounded-lg">
          <h2 class="text-xl font-bold mb-4">Subscribe to our Newsletter</h2>
          <p class="mb-4">
            Stay updated with the latest articles, tips, and exclusive promotions. Subscribe to our newsletter and never miss out on the best content for AI lovers!
          </p>
          <form class="flex flex-col gap-4">
            <input
              type="email"
              placeholder="Enter your email"
              class="p-3 border border-gray-300 rounded-lg"
              required
            />
            <button
              type="submit"
              class="p-3 bg-black text-white font-bold rounded-lg hover:bg-gray-800 flex items-center justify-center gap-2"
            >
              <Mail class="h-5 w-5" />

              Subscribe
            </button>
          </form>
        </div>
      </aside>
    </div>
  </section>
</DefaultLayout>
